{% extends "WtsdaCoreBundle::base.html.twig" %}

{% block head_css %}
    {{ parent() }}
    {% stylesheets 'bundles/wtsdaapp/css/*' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block body_container_header %}
    <div class="row-fluid">
        <div class="span2">
            <img style="width:100px; height: 100px;" src="{{ asset('bundles/wtsdaapp/images/logos/wtsda_129x129.png') }}" />
        </div>
        <div class="span8 text-right">
            <h1 class="muted">WTSDA Region 1</h1>
            <p class="muted">
            A proud member of the World Tang Soo Do Association<br />
            Traditionalism. Professionalism. Brotherhood.
            </p>
        </div>
        <div class="span2 text-right">
            <img data-src="holder.js/140x140" class="img-circle" alt="100x100" style="width: 100px; height: 100px;" src="data:image/png;base64,">
        </div>
    </div>
    <div class="masthead">
        <div class="navbar">
            <div class="navbar-inner">
                <div class="container">
                    <ul class="nav">
                        <li class="active"><a href="{{ path('wtsda_app_index') }}">Latest</a></li>
                        <li><a href="{{ path('wtsda_app_events') }}">Events</a></li>
                        <li><a href="#">Schools</a></li>
                        <li><a href="#">Scholarship</a></li>
                        <li><a href="#">Media</a></li>
                        <li><a href="#">About</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

{% endblock %}

{% block body_container_footer %}
<div class="pull-left">&copy; 2013 WTSDA Region 1</div>
<div class="pull-right">Contact Us <i class="icon-envelope"></i></div>
{% endblock %}

